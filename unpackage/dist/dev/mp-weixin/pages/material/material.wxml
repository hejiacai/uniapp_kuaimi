<view class="boy_box"><view class="titleB">请完善以下资料</view><view class="material_list"><view class="photo"><block wx:if="{{photoImg&&photoImg.length>0}}"><image src="{{photoImg}}" mode data-event-opts="{{[['tap',[['upPhoto',['$event']]]]]}}" bindtap="__e"></image></block><block wx:else><image src="../../static/img/head.png" mode data-event-opts="{{[['tap',[['upPhoto',['$event']]]]]}}" bindtap="__e"></image></block><view>点击上传头像</view></view><view class="certifi-item"><text class="title">姓名</text><view class="input"><input type="text" id="txtUserName" placeholder="请输入姓名" placeholder-class="placeholderStyle" data-event-opts="{{[['input',[['__set_model',['$0','user_name','$event',[]],['resumeList']]]]]}}" value="{{resumeList.user_name}}" bindinput="__e"/></view></view><view class="certifi-item"><text class="title">性别</text><view class="secInput"><picker style="width:100%;" value="{{obj.genderT}}" range="{{sexArray}}" data-event-opts="{{[['change',[['pickerChangeVal',['$event','genderT']]]]]}}" bindchange="__e"><block wx:if="{{sexPickerView}}"><view class="qxz">请选择</view></block><block wx:else><view class="secBox">{{sexArray[obj.genderT]}}</view></block></picker><view class="icon iconfont icon-morex"></view></view></view><view class="certifi-item"><text class="title">出生日期</text><view class="secInput"><picker style="width:100%;" mode="date" value="{{date}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><block wx:if="{{sexbirthdayView}}"><view class="qxz">请选择</view></block><block wx:else><view class="secBox">{{resumeList.birthday}}</view></block></picker><view class="icon iconfont icon-morex"></view></view></view><view class="certifi-item"><text class="title">现居地址</text><view data-event-opts="{{[['tap',[['goAddress',['$event']]]]]}}" class="secInput" bindtap="__e"><block wx:if="{{curPView}}"><view class="qxz">请选择</view></block><block wx:else><view class="secBox">{{resumeList.cur_area_name}}</view></block><view class="icon iconfont icon-morex"></view></view></view><view class="certifi-item"><text class="title">学历</text><view class="secInput"><picker style="width:100%;" value="{{obj.edu}}" range="{{material.education}}" range-key="name" data-event-opts="{{[['change',[['pickerChangeVal',['$event','edu']]]]]}}" bindchange="__e"><block wx:if="{{educationView}}"><view class="qxz">请选择</view></block><block wx:else><view class="secBox">{{material.education[obj.edu].name}}</view></block></picker><view class="icon iconfont icon-morex"></view></view></view><view class="certifi-item"><text class="title">身高</text><view class="secInput"><picker style="width:100%;" value="{{obj.height}}" range="{{material.stature}}" range-key="value" data-event-opts="{{[['change',[['pickerChangeVal',['$event','height']]]]]}}" bindchange="__e"><block wx:if="{{statureView}}"><view class="qxz">请选择</view></block><block wx:else><view class="secBox">{{material.stature[obj.height].value}}</view></block></picker><view class="icon iconfont icon-morex"></view></view></view><view class="certifi-item"><text class="title">体重</text><view class="input"><input id="weight" name="weight" placeholder="请输入体重(kg)" placeholder-class="placeholderStyle" type="number" data-event-opts="{{[['input',[['__set_model',['$0','weight','$event',[]],['resumeList']]]]]}}" value="{{resumeList.weight}}" bindinput="__e"/></view></view><view class="center_g"></view><view class="certifi-item"><text class="title">期望职位</text><view class="secInput"><view data-event-opts="{{[['tap',[['goJobCategory',['$event']]]]]}}" class="secBox" bindtap="__e"><block wx:if="{{jobsortView}}"><view class="qxz">请选择</view></block><block wx:else><view>{{resumeList.str_expect_jobsorts}}</view></block></view><view class="icon iconfont icon-morex"></view></view></view><view class="certifi-item"><text class="title">期望薪资</text><view class="secInput"><picker style="width:100%;" value="{{obj.money}}" range="{{material.moneyList}}" range-key="name" data-event-opts="{{[['change',[['pickerChangeVal',['$event','money']]]]]}}" bindchange="__e"><block wx:if="{{moneyListView}}"><view class="qxz">请选择</view></block><block wx:else><view class="secBox">{{material.moneyList[obj.money].name}}</view></block></picker><view class="icon iconfont icon-morex"></view></view></view><view class="certifi-item"><text class="title">工作地点</text><view class="secInput"><view data-event-opts="{{[['tap',[['workAddress',['$event']]]]]}}" class="secBox" bindtap="__e"><block wx:if="{{areasListView}}"><view class="qxz">请选择</view></block><block wx:else><view>{{resumeList.str_expect_areas}}</view></block></view><view class="icon iconfont icon-morex"></view></view></view></view><view class="btn"><view data-event-opts="{{[['tap',[['savePerInfo',['$event']]]]]}}" bindtap="__e">下一步</view></view></view>