<view data-event-opts="{{[['tap',[['navigateJobDetail',['$0'],['jobItem.job_flag']]]]]}}" class="jobItem data-v-4e9b5bfa" bindtap="__e"><block wx:if="{{jobItem.is_today_issue&&$root.m0}}"><view class="icon iconfont icon-home_new fresh_signnew data-v-4e9b5bfa"></view></block><view class="jobItemTitle data-v-4e9b5bfa"><view class="jobItemName wordEllipsis data-v-4e9b5bfa">{{jobItem.station}}</view><view class="jobSalary data-v-4e9b5bfa">{{jobItem.salary_text}}</view></view><view class="jobItemCompany data-v-4e9b5bfa"><view class="jobItemCompany-top data-v-4e9b5bfa"><text class="jobItemCompany-name data-v-4e9b5bfa">{{jobItem.company_shortname}}</text><block wx:if="{{nearbyJob}}"><text class="jobItemCompany-refreshTime data-v-4e9b5bfa">{{jobItem.distant}}</text></block><block wx:else><text class="jobItemCompany-refreshTime data-v-4e9b5bfa">{{jobItem.refresh_time}}</text></block></view><block wx:if="{{jobItem.is_famous=='1'}}"><text class="sign famousCompany data-v-4e9b5bfa">名企</text></block><block wx:if="{{jobItem.is_award=='1'}}"><text class="sign bonusMoney data-v-4e9b5bfa">{{"奖"+jobItem.award_mount}}</text></block><block wx:for="{{jobItem.company_tags}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{jobItem.company_tags.length>0}}"><text class="sign topCompany data-v-4e9b5bfa">{{item}}</text></block></block></view><view class="jobItemMap data-v-4e9b5bfa">{{''+jobItem.area_name}}<text class="data-v-4e9b5bfa">|</text>{{jobItem.age_sex_text+''}}</view><view class="jobItemWarfe data-v-4e9b5bfa"><block wx:for="{{jobItem.rewards}}" wx:for-item="rewardItem" wx:for-index="index" wx:key="index"><block wx:if="{{index<4}}"><text class="data-v-4e9b5bfa">{{rewardItem}}</text></block><block wx:else><block wx:if="{{index==4}}"><text class="data-v-4e9b5bfa">...</text></block></block></block></view></view>